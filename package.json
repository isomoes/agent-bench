{
  "name": "agent-bench",
  "version": "0.2.0",
  "description": "An open-source benchmark for evaluating AI coding agents on real-world engineering tasks",
  "type": "module",
  "bin": {
    "agent-bench": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@opencode-ai/sdk": "^1.0.166",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.0",
    "simple-git": "^3.25.0",
    "csv-stringify": "^6.5.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "bun-types": "latest",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "agent",
    "benchmark",
    "ai",
    "coding",
    "opencode",
    "evaluation"
  ],
  "author": "Agent Bench Contributors",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  }
}
